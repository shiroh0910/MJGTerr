{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/sw.js", "destination": "/kill-sw.js" },
    { "source": "/src/sw.js", "destination": "/kill-sw.js" }
  ]
}